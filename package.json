    "glob": "^8.0.3",
    "jest": "^29.0.0",
    "js-yaml": "^4.1.0",
    "make-coverage-badge": "^1.2.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@tunnelhub/sdk": "^1.3.4"
  }
